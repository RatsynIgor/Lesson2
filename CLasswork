class Subject: #Описание класса
    name = str
    level = str
    hours = int
    isStudy = bool

    dev init(self, name, level, hours=18, isStudy):
        self.name = name
        self.level = level
        self.hours = hours
        self.isStudu = isStudy

pySenior = Subject("Python", "Senior", True) #Надавання значень
print(pySenior.name, pySenior.level, pySenior.hours) #вивід классу

